{
  "manifest_version": 2,
  "name": "Google Background Random",
  "version": "0.1.0",
  "icons": {
    "16": "./img/icon16.png",
    "48": "./img/icon48.png",
    "128": "./img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/",
        "https://www.google.de/",
        "https://www.google.at/"
      ],
      "css": ["style.css"]
    }
  ]
}
